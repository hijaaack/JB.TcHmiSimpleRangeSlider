        "TcHmi.Controls.Beckhoff.TcHmiBarChart.ChartAxisLabeling": {
            "title": "ChartAxisLabeling",
            "type": "string",
            "enum": ["Number", "Scientific", "Auto"],
            "default": "Number"
        },
